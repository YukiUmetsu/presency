<h2>Show Post</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @post.title %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @post.content %>
  </li>

  <li>
    <strong>Publicity:</strong>
    <%= @post.publicity %>
  </li>

</ul>

<span><%= link "Edit", to: admin_post_path(@conn, :edit, @post) %></span>
<span><%= link "Back", to: admin_post_path(@conn, :index) %></span>
